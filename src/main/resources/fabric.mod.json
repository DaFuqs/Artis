{
  "schemaVersion": 1,
  "id": "artis",
  "version": "$version",

  "name": "Artis",
  "icon": "assets/artis/icon.png",
  "description": "Customizable crafting tables for modpacks",
  "license": "MIT",
  "contact": {
    "sources": "https://github.com/AllOfFabric/Artis"
  },
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.alloffabric.artis.Artis"
    ],
    "client": [
      "io.github.alloffabric.artis.ArtisClient"
    ],
    "rei_plugins_v0": [
      "io.github.alloffabric.artis.compat.rei.ArtisREIPlugin"
    ],
	"rei_containers": [
      "io.github.alloffabric.artis.compat.rei.ArtisServerContainerPlugin"
    ],
    "kubejs-init": [
	  "io.github.alloffabric.artis.compat.kubejs.ArtisKubeJS"
    ]
  },
  "mixins": [
    "mixin.artis.compat.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  }
}
